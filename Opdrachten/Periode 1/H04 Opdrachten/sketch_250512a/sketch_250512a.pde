void setup() {
  // Calculate the modulo values
  int result1 = 15 % 2;
  int result2 = 31 % 3;
  int result3 = 10 % 5;
  int result4 = 38 % 4;

  // Print the results to the console
  println("15 % 2 = " + result1);
  println("31 % 3 = " + result2);
  println("10 % 5 = " + result3);
  println("38 % 4 = " + result4);
}
